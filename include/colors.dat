# Steuerungen
    clr='\e[0m' ;export clr
    bold='\e[1m' ;export bold
    mild='\e[2m' ;export mild # Wenig Kontrast? Keine Ahnung
    underline='\e[4m' ;export underline
    blink='\e[5m' ;export blink
    reverse='\e[7m' ;export reverse
#
# Alle Farben anzeigen
    # https://colors.sh/
formatierungen() {
    # Einige Formatierungen
    for clbg in {40..47} {100..107} 49 ; do
        #Foreground
        for clfg in {30..37} {90..97} 39 ; do
            #Formatting
            for attr in 0 1 2 4 5 7 ; do
                #Print the result
                echo -en "\e[${attr};${clbg};${clfg}m ^[${attr};${clbg};${clfg}m \e[0m"
            done
            echo #Newline
        done
    done
}
function fill_up {
    str=""
    size=${#1}
    while ((size<$2)); do
        str+=" "
        size=$((size+1));
    done
    str+="$1"
    echo "$str"
}
vordergrund_mit_name() {
    #Vordergrund auf Default
    i=0
    while ((i<256)); do
        colorname=$(fill_up "${_COLOR_NAMES[$i]}" 18)
        no=$(fill_up "$i" 4)
        #shellcheck disable=2027 disable=2248
        echo -en "\e[38;5;"$i"m $colorname $no $clr";
        #shellcheck disable=2292
        if [ $(((i + 1) % 6)) == 4 ] ;
            then echo; #6 Pro Zeile
        fi;
        i=$((i+1));
    done
}
vordergrund() {
    #Vordergrund auf Default
    for color in {0..255} ; do
        printf "\e[38;5;%sm %3s \e[0m" "$color"  "$color"
        #shellcheck disable=2292
        if [ $(((color + 1) % 16)) == 0 ] ; then echo; fi; #16 Pro Zeile
    done
}
vordergrund_fett_auf_schwarz() {
    #Vordergrund auf Schwarz Fett
    for color in {0..255} ; do
        printf "\e[1m\e[48;5;16m\e[38;5;%sm%3s \e[0m" "$color" "$color"
        #shellcheck disable=2292
        if [ $(((color + 1) % 16)) == 0 ] ; then echo; fi; #16 Pro Zeile
    done
}
hintergrund_mit_name() {
    #Default auf Hintergrund
    i=0
    while ((i<256)); do
        colorname=$(fill_up "${_COLOR_NAMES[$i]}" 18)
        no=$(fill_up "$i" 4)
        #shellcheck disable=2027 disable=2248
        echo -en "\e[48;5;"$i"m $colorname $no $clr";
        #shellcheck disable=2292
        if [ $(((i + 1) % 6)) == 4 ] ; then
            echo; #6 Pro Zeile
        fi;
        i=$((i+1));
    done
}
hintergrund() {
    #Default auf Hintergrund
    for color in {0..255} ; do
        printf "\e[48;5;%sm%3s \e[0m" "$color" "$color"
        #shellcheck disable=2292
        if [ $(((color + 1) % 16)) == 0 ] ; then echo; fi; #16 Pro Zeile
    done
}
hintergrund_mit_weiss() {
    #Weiss auf Hintergrund
    for color in {0..255} ; do
        printf "\e[38;5;15m\e[48;5;%sm%3s \e[0m" "$color" "$color"
        #shellcheck disable=2292
        if [ $(((color + 1) % 16)) == 0 ] ; then echo; fi; #16 Pro Zeile
    done
};export hintergrund_mit_weiss
